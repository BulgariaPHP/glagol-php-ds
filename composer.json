{
  "name": "bulgaria-php/glagol-php-ds",
  "description": "Internal Glagol library wrapping php-ds",
  "minimum-stability": "dev",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Yoan-Alexander Grigorov",
      "email": "joan.grigorov@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {"Glagol\\Ds\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"GlagolTest\\Ds\\": "test/"}
  },
  "require-dev": {
